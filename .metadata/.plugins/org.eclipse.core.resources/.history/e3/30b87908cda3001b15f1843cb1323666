import java.util.ArrayList;

public class Button {

	public static void main(String[] args) {
		ArrayList<String> list_str = new ArrayList<String>();
		
		list_str.add("one");
		list_str.add("two");
		list_str.add("three");
		
		// 일반적인 for문
		for (int i = 0; i < list_str.size(); i++) {
			System.out.println(list_str.get(i));
		}

	}

}
